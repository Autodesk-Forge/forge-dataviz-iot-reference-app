        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "6.24.1",
        "concurrently": "^3.5.1",
        "css-loader": "^0.28.8",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "lerna": "^3.22.1",
        "local-web-server": "~0.5.23",
        "node-sass": "^4.7.2",
        "node-loader": "^1.0.2",
        "nodemon": "^1.14.11",
